body{
  /* IE10 */ 
  background-image: -ms-linear-gradient(top, #696969 0%, #000061 100%);

  /* Mozilla Firefox */ 
  background-image: -moz-linear-gradient(top, #696969 0%, #000061 100%);

  /* Opera */ 
  background-image: -o-linear-gradient(top, #696969 0%, #000061 100%);

  /* Webkit (Safari/Chrome 10) */ 
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #696969), color-stop(1, #000061));

  /* Webkit (Chrome 11+) */ 
  background-image: -webkit-linear-gradient(top, #696969 0%, #000061 100%);

  /* Proposed W3C Markup */ 
  background-image: linear-gradient(top, #696969 0%, #000061 100%);

  width: 900px;
  margin: 0 auto;
  min-height: 100%;
  height: 100%;
}

#posts DIV.postl{
  display: table-cell;
  vertical-align: top;
  text-align: right;
  width: 80%;
}

#posts DIV.posta{
  width: 20%;
  display: table-cell;
}

UL#posts LI{
  display: table;
  padding-top: 11px;
  width: 100%;
}

H1{
  font-size: 26.4pt;
}

NAV{
  position: absolute;
  left: 0;
  width: 100%;
}

NAV UL{
  margin: 0 auto;
  width: 900px;
  list-style: none;
}

NAV UL LI{
  float: left;
}

NAV UL LI A{
  display: block;
  margin-right: 20px;
  width: 140px;
  font-size: 14px;
  line-height: 44px;
  text-align: center;
  text-decoration: none;
  color: #777;
  background-color: #000000;
  margin-top: 6px;
  border-radius: 10px;
}

